sword = () => {\r\n    setPasswordValid(passwordInputRef.current.value.trim().length > 6);\r\n  };\r\n  return (\r\n    <div className=\"bg\">\r\n      <div className=\"login-container\">\r\n        <div className=\"login-background\"></div>\r\n\r\n        <form className=\"login\" onSubmit={submitHandler}>\r\n          <button className=\"back\" onClick={navigateTo}>\r\n            {\"X\"}\r\n          </button>\r\n          <h2>LOGIN</h2>\r\n          <br />\r\n          <input\r\n            type=\"text\"\r\n            className={usernameValid ? \"login-input\" : \"login-input invalid\"}\r\n            placeholder=\"Username\"\r\n            onBlur={validateUsername}\r\n            ref={usernameInputRef}\r\n          />\r\n          {!userAvailable ? <p className=\"error\">{errorMessage}</p> : <br />}\r\n          <input\r\n            type=\"password\"\r\n            className={passwordValid ? \"login-input\" : \"login-input invalid\"}\r\n            placeholder=\" Password\"\r\n            onBlur={validatePassword}\r\n            ref={passwordInputRef}\r\n          />\r\n          <br />\r\n\r\n          <button type=\"submit\" disabled={!formIsValid}>\r\n            Log in\r\n          </button>\r\n          <br />\r\n          <a href=\"/reset\" className=\"login-link\">\r\n            CAN'T SIGN IN ?\r\n          </a>\r\n          <a href=\"/register\" className=\"login-link\">\r\n            CREATE ACCOUNT\r\n          </a>\r\n          <br />\r\n        </form>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n","C:\\Users\\FPT SHOP\\Documents\\VSCode\\Pet-Shop\\pet-shop\\src\\components\\account\\Profile.js",["199"],"import React, { useState } from \"react\";\r\nimport Headerwhite from \"../layouts/Header_white\";\r\nimport Footerwhite from \"../layouts/Footer_white\";\r\nimport ProfileTab from \"./ProfileTab\";\r\nimport ProfileInfo from \"./ProfileInfo\";\r\nimport ProfileBody from \"./ProfileBody\";\r\nimport { useHistory } from \"react-router-dom\";\r\ni